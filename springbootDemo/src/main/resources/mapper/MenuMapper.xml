<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.examplorfotg.springbootdemo.mapper.MenuMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.examplorfotg.springbootdemo.entity.Menu">
        <id column="Id" property="Id" />
        <result column="menuCode" property="menucode" />
        <result column="menuName" property="menuname" />
        <result column="menuLevel" property="menulevel" />
        <result column="parentMenuCode" property="parentmenucode" />
        <result column="click" property="menuclick" />
        <result column="roleId" property="roleId" />
        <result column="menuComponent" property="menucomponent" />
        <result column="menuIcon" property="menuicon" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        Id, menuCode, menuName, menuLevel, parentMenuCode,click, roleId, menuComponent, menuIcon
    </sql>

</mapper>
